  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "start:prod": "export NODE_ENV=production || SET \"NODE_ENV=production\" && nodemon server.js",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && jest --watchAll",
    "debug": "ndb server.js",
    "debug:prod": "export NODE_ENV=production || SET \"NODE_ENV=production\" && ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
